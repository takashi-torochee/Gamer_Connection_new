
<h1>会員一覧</h1>

<% @user.each do |users| %>

  <div style="margin:20px">
      <%= users.id %>
      
      <%= link_to edit_admin_user_path(users.id) do %>
      <%= users.name %>
      <% end %>
      
      <%= users.age %>
      <%= users.email %>
      <%= users.is_active %>
  </div>
  
<% end %>
